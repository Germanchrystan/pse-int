Algoritmo sexto
	Definir a, b Como Entero;
	Escribir "Ingrese el primer entero: ";
	Leer a;
	Escribir "Ingrese el segundo entero: ";
	Leer b;
	
	Escribir "a + b = ", a + b;
	Escribir "a / b = ", a / b;
	Escribir "a - b = ", a - b;
	Escribir "a * b = ", a * b;
	Escribir "a ^ b = ", a ^ b;
	Escribir "a % b = ", a % b;
	
FinAlgoritmo
